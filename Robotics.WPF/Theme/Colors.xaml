<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="VeryDark">#0D0D0D</Color>
    <SolidColorBrush x:Key="VeryDarkBrush" Color="{StaticResource VeryDark}" />

    <Color x:Key="ViewerBackground">#121212</Color>
    <SolidColorBrush x:Key="ViewerBackgroundBrush" Color="{StaticResource ViewerBackground}" />


    <Color x:Key="SidePanelBackground">#101010</Color>
    <SolidColorBrush x:Key="SidePanelBackgroundBrush" Color="{StaticResource SidePanelBackground}" />

    <Color x:Key="PanelBackground">#161616</Color>
    <SolidColorBrush x:Key="PanelBackgroundBrush" Color="{StaticResource PanelBackground}" />

    <Color x:Key="DarkPanelBackground">#050505</Color>
    <SolidColorBrush x:Key="DarkPanelBackgroundBrush" Color="{StaticResource DarkPanelBackground}" />

    <Color x:Key="C1">#1C1C1C</Color>
    <SolidColorBrush x:Key="C1Brush" Color="{StaticResource C1}" />

    <Color x:Key="White2">#DFDFDF</Color>
    <SolidColorBrush x:Key="White2Brush" Color="{StaticResource White2}" />

    <Color x:Key="Accent">#0071B0</Color>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource Accent}" />

    <Color x:Key="Accent2">#30B3FC</Color>
    <SolidColorBrush x:Key="Accent2Brush" Color="{StaticResource Accent2}" />


</ResourceDictionary>